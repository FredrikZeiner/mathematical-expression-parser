EXPRESSION 	::=		TERM ((PLUS | MINUS) TERM)+;
TERM 				::=		FACTOR ((MULTIPLY |Â DIVIDE) FACTOR)*;
FACTOR 			::=		INT | FLOAT;
INT 				::=		DIGIT+;
FLOAT 			::=		DIGIT+ , '.' , DIGIT+;
PLUS				::=		'+';
MINUS				::=		'-';
MULTIPLY		::=		'*';
DIVIDE			::=		'/';
